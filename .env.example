# Copy this file to ".env" and fill with strong values.
# Never commit your ".env".

# Compose
COMPOSE_PROJECT_NAME=art-insitut

# Kimai database credentials
KIMAI_DB_ROOT_PASSWORD=change_me_strong_root
KIMAI_DB_NAME=kimai
KIMAI_DB_USER=kimai
KIMAI_DB_PASSWORD=change_me_strong
# URL-encoded variant of the Kimai password (used in DATABASE_URL)
KIMAI_DB_PASSWORD_URLENC=change_me_strong_urlencoded

# Kimai outbound mail (SMTP)
KIMAI_MAILER_DSN=smtp://user%40example.com:password@hostname:465/?encryption=ssl&auth_mode=login
KIMAI_MAILER_FROM=no-reply@example.com

# Nextcloud database credentials
NEXTCLOUD_DB_ROOT_PASSWORD=change_me_strong_root
NEXTCLOUD_DB_NAME=nextcloud
NEXTCLOUD_DB_USER=nextcloud
NEXTCLOUD_DB_PASSWORD=change_me_strong
# URL-encoded variant of Nextcloud DB password
NEXTCLOUD_DB_PASSWORD_URLENC=change_me_strong_urlencoded

# Redis password used by Nextcloud for locking
REDIS_PASSWORD=change_me_strong

# TURN/STUN settings for Nextcloud Talk
TURN_DOMAIN=turn.example.com
TURN_REALM=example.com
TURN_PUBLIC_IP=203.0.113.10
TURN_SHARED_SECRET=change_me_shared_secret_hex
TURN_MIN_PORT=49160
TURN_MAX_PORT=49200
